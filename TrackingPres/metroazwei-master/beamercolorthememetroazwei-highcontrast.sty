\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthememetroazwei-highcontrast}[2017/12/07 metroazwei color theme]
\usecolortheme{metroazwei}

\definecolor{mAlert}{HTML}{AD003D}
\definecolor{mExample}{HTML}{005580}

\setbeamercolor{normal text}{%
  fg=black,
  bg=white
}
\setbeamercolor{alerted text}{%
  fg=mAlert,
}
\setbeamercolor{example text}{%
  fg=mExample,
}
\mode<all>
\endinput
